{
    "files.exclude": {
        "**/.vscode": true
    },
    "thunder-client.saveToWorkspace": true,
    "thunder-client.autoRunCurl": false,
    "thunder-client.requestLayout": "Top/Bottom",
    "thunder-client.saveRequestOnSend": false,
    "mdb.connectionSaving.defaultConnectionSavingLocation": "Workspace",
    "mdb.sendTelemetry": false,
    "mdb.useDefaultTemplateForPlayground": true,
    "mongodbLanguageServer.trace.server": {},
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "editor.tabCompletion": "on",
    "thunder-client.sidebar.defaultTab": "Collections",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 2,
}